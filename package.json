{
  "name": "starter-kit",
  "version": "1.0.0",
  "description": "Atecna Academy starter kit",
  "author": "ATECNA",
  "license": "MIT",
  "main": "src/main.js",
  "scripts": {
    "preview": "vite --port 3333",
    "dev": "yarn run preview --open",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:e2e:run": "npx cypress run",
    "test:unit": "vitest run",
    "test:unit:coverage": "yarn run test:unit --coverage",
    "mock": "node node_modules/@staticdeploy/mock-server/src/bin/index.js --watch"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@staticdeploy/mock-server": "3.0.0",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.44",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitest/coverage-c8": "^0.23.4",
    "autoprefixer": "^10.4.7",
    "c8": "^7.11.3",
    "cypress": "^10.9.0",
    "eslint": "^8.16.0",
    "eslint-plugin-cypress": "^2.12.1",
    "jsdom": "^20.0.1",
    "postcss": "^8.4.14",
    "sass": "^1.52.1",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4",
    "vite": "^3.1.4",
    "vitest": "^0.23.4"
  }
}
